{
    "name": "welberty.martins/wam-crypto-middleware-php",
    "version": "1.0.0",
    "description": "Basic library for encoding, decoding, hashing and encryption.",
    "autoload": {
        "psr-4": {
            "Wam\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Test\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "welbertymartins",
            "email": "38479002+welbertymartins@users.noreply.github.com"
        }
    ],
    "scripts": {
        "test": "./vendor/bin/phpunit",
        "psr-list" : "./vendor/bin/phpcs --standard=PSR12 src/",
        "psr-apply": "./vendor/bin/phpcbf --standard=PSR12 src/",
        "psr-list-tests" : "./vendor/bin/phpcs --standard=PSR12 tests/",
        "psr-apply-tests": "./vendor/bin/phpcbf --standard=PSR12 tests/"
    },
    "require": {
        "php": "8.1.*"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "squizlabs/php_codesniffer": "^3"
    }
}
